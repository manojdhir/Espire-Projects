                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
	P.MaterialNumber AS MaterialNumber,
	CAST(PP.ProductId as nvarchar) + CAST(PP.CountryId as nvarchar) AS ProductPriceKey,
	PP.DistributorDirect60,
	PP.Valid_From,
	PP.Valid_Until
FROM ProductPrice PP
INNER JOIN Product P ON P.ProductId = PP.ProductId
INNER JOIN Country C ON C.CountryId = PP.CountryId
WHERE
	C.Country = 'Germany'
	AND PP.DistributorDirect60 IS NOT NULL</property>
