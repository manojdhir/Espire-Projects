                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">With tblPromoCode AS
(SELECT distinct 
	 CASE 
		WHEN PC.PromoCode LIKE '%[a-z0-9]%' THEN UPPER(dbo.DQRemoveNonAlphaNumericCharacters('%[^a-z0-9]%',PC.PromoCode))
	   ELSE 
		UPPER(PC.PromoCode) END as AlphaNumericPromoCode
	,UPPER(PC.PromoCode) as PromoCode
	,PC.MaterialNumber
	,PC.Currency
	,CAST(dbo.ToNumeric(PC.[Discounted Price]) as Float) as DiscountedPrice
	,CAST(dbo.ToNumeric(PC.[Discounted Price]) as float)/(select top 1 fpp.ExchangeRate from BIOS4_DW.dbo.FactProductPrice fpp where fpp.ProductKey = PC.MaterialNumber 
			and fpp.ProductPriceDistributorDirect60CurrId = DC.CurrencyId AND ((cast(SUBSTRING(PC.Startdate,7,4)+SUBSTRING(PC.Startdate,4,2)+SUBSTRING(PC.Startdate,1,2) as date) 
										between cast(fpp.ProductPriceValidityFrom as Date) and cast(isnull(fpp.ProductPriceValidityUntil,getdate()+11000) as date))
										OR
										(cast(SUBSTRING(PC.Enddate,7,4)+SUBSTRING(PC.Enddate,4,2)+SUBSTRING(PC.Enddate,1,2) as date) 
										between cast(fpp.ProductPriceValidityFrom as Date) and cast(isnull(fpp.ProductPriceValidityUntil,getdate()+11000) as date)))
				Order by ProductPriceValidityFrom desc) as DiscountedPrice_EUR
	,CAST(ISNULL(PC.Limit,0) as Int) as Limit
	,PC.StartDate
	,PC.EndDate
  FROM [BIOS4_Staging].[dbo].[PromoCode] PC
  INNER JOIN BIOS4_DW.dbo.DimCurrency DC ON DC.CurrencyKey = PC.Currency
  ),

  tempPromoCode AS (
SELECT 
	 ROW_NUMBER()OVER (PARTITION BY DP.ProductId,DP.ProductKey,PC.PromoCode,PP.ProductPriceDistributorDirect60CurrId ORDER BY PP.ProductPriceValidityFrom DESC) as RowId
	,PC.AlphaNumericPromoCode
	,DP.ProductId
	,DP.ProductKey
	,PC.PromoCode
	--,PP.CountryId
	,PP.ProductPriceDistributorDirect60CurrId as CurrencyId
	,PC.DiscountedPrice_EUR*PP.ExchangeRate as DiscountedPrice_Local
	,DiscountedPrice_EUR
	,PC.Limit
	,PP.ExchangeRate
	,PP.ProductPriceValidityFrom
	,PP.ProductPriceValidityUntil
  FROM tblPromoCode PC
INNER JOIN BIOS4_DW.dbo.DimProduct DP ON DP.ProductKey = PC.MaterialNumber and DP.ProductEndDate is null
INNER JOIN BIOS4_DW.dbo.FactProductPrice PP ON PP.ProductKey = DP.ProductKey
AND ((cast(SUBSTRING(PC.Startdate,7,4)+SUBSTRING(PC.Startdate,4,2)+SUBSTRING(PC.Startdate,1,2) as date) 
	between cast(PP.ProductPriceValidityFrom as Date) and cast(isnull(PP.ProductPriceValidityUntil,getdate()+11000) as date))
	OR
	(cast(SUBSTRING(PC.Startdate,7,4)+SUBSTRING(PC.Startdate,4,2)+SUBSTRING(PC.Startdate,1,2) as date) 
	between cast(PP.ProductPriceValidityFrom as Date) and cast(isnull(PP.ProductPriceValidityUntil,getdate()+11000) as date)))
AND PP.CountryId &lt;&gt; 102 
AND (PP.ProductPriceDistributorDirect60CurrId&lt;&gt;14 OR (PP.ProductPriceDistributorDirect60CurrId = 14 AND PP.ExchangeRate &lt;&gt; '5.0000')) --and dp.Productid= 120662
)


Select 
	 tmp.AlphaNumericPromoCode
	,tmp.ProductId
	,tmp.ProductKey
	,tmp.PromoCode
	--,tmp.CountryId
	,tmp.CurrencyId
	,DiscountedPrice_Local
	,DiscountedPrice_EUR
	,Limit
	,ExchangeRate
from
	tempPromoCode tmp
Where RowId = 1</property>
