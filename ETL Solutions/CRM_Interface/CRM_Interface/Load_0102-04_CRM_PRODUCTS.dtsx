        <SQLTask:SqlTaskData
          SQLTask:Connection="{4508193A-7E78-4A3C-9B0A-5507F14E90FF}"
          SQLTask:SqlStatementSource="update a set &#xA;a.ITDACTIVE = 0,&#xA;a.OKI_UPDATETIMESTAMP_BI = getdate()&#xA;from [dbo].[0102-04_CRM_PRODUCTS] a&#xA;left join &#xA;(&#xA;select &#xA;a.MaterialNumber as BPRNUMBER,&#xA;CAST(ISNULL (C.ACTIVE,0) AS INT) as ITDACTIVE,&#xA;f.CountryCode as OKI_COUNTRY,&#xA;ISNULL(e.Language,'English') as OKI_LANGUAGE&#xA;from Product a&#xA;left join ProductCountry c on a.ProductId = c.ProductId&#xA;left join Country f on c.CountryId = f.CountryId&#xA;left join CountryLanguage d on c.CountryId = d.CountryId&#xA;left join Language e on d.LanguageId = e.LanguageId&#xA;join ProductPrice pp on a.ProductId = pp.ProductId and c.CountryId = pp.CountryId&#xA;where f.CountryCode is not null and f.Active = 1 and c.Active = 1&#xA;and pp.ActiveCheckf = 1&#xA;) b on a.BPRNUMBER = b.BPRNUMBER&#xA;and a.OKI_COUNTRY = b.OKI_COUNTRY and a.OKI_LANGUAGE = b.OKI_LANGUAGE&#xA;and a.ITDACTIVE = b.ITDACTIVE&#xA;where b.BPRNUMBER is null&#xA;and a.ITDACTIVE = 1" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
