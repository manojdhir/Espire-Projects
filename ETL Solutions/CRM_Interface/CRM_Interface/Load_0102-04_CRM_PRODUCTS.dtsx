              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select 
a.MaterialNumber as BPRNUMBER,
a.Product as KEYWORD,
m.ProductTypeName as OKI_PRODUCTTYPE,
n.ProductGroupName as OKI_PRODUCTGROUP,
o.ProductFamilyName as OKI_PRODUCTFAMILY,
p.ProductSubTypeName as OKI_PRODUCTSUBTYPE,
b.Category as CATEGORY,
i.Segment as OKI_PRODUCTSEGMENT,
CAST(ISNULL (C.ACTIVE,0) AS BIT) as ITDACTIVE,
a.RelatedProducts as OKI_RELATEDPRODUCTS,
CAST(c.Modelnamesf as NVARCHAR(2000)) as OKI_MODELNAMESF,
CONVERT(DATETIME, CONVERT(VARCHAR(19), getdate(), 120)) as OKI_UPDATETIMESTAMP_BI,
f.CountryCode as OKI_COUNTRY,
ISNULL(e.Language,'English') as OKI_LANGUAGE,
k.Translation as OKI_KEYWORD_LOCAL,
a.Product as DESCRIPTION,
g.Translation as OKI_CATEGORY_LOCAL,
j.Translation as OKI_PRODUCTSEGMENT_LOCAL,
CONCAT(a.MaterialNumber,' -',f.country,' -',e.Language) as OKI_KEY
from Product a
left join Category b on a.CategoryId = b.CategoryId
left join ProductCountry c on a.ProductId = c.ProductId
left join CountryLanguage d on c.CountryId = d.CountryId
left join Language e on d.LanguageId = e.LanguageId
left join Country f on c.CountryId = f.CountryId
left join CategoryTranslation g on g.CategoryId = a.CategoryId and e.LanguageId = g.LanguageId
left join ProductSegment h on a.ProductId = h.ProductId
left join Segment i on h.SegmentId = i.SegmentId
left join SegmentTranslation j on j.SegmentId = h.SegmentId and j.LanguageId = e.LanguageId
left join ProductTranslation k on k.ProductId = a.ProductId and k.LanguageId = e.LanguageId
left join (SELECT * FROM DimProduct WHERE ProductEndDate IS NULL) l on a.MaterialNumber = l.ProductKey
left join DimProductType m on l.ProductTypeid = m.ProductTypeId
left join DimProductGroup n on l.ProductGroupId = n.ProductGroupId
left join DimProductFamily o on l.ProductFamilyId = o.ProductFamilyId
left join DimProductSubType p on l.ProductSubTypeId = p.ProductSubTypeId
join ProductPrice pp on a.ProductId = pp.ProductId and c.CountryId = pp.CountryId
where f.CountryCode is not null and f.Active = 1 and c.Active = 1
and pp.ActiveCheckf = 1</property>
