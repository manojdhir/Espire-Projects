                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">DECLARE @BatchId bigint
DECLARE @CreatedBy nvarchar(100)
SET @BatchId=?
SET @CreatedBy=?

